package testNG;

import org.testng.annotations.DataProvider;

public class DataProviderEg2 
{
	@DataProvider
	public Object[][] data()
	{
		Object[][] d=new Object[3][2];
		
		d[0][0]="firefox";
		d[0][1]="http://google.com";
		

		d[1][0]="chrome";
		d[1][1]="http://gmail.com";

		d[2][0]="ie";
		d[2][1]="http://facebook.com";
		
		return d;
	}
}
